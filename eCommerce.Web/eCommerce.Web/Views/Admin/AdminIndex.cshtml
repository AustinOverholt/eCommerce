@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<h1 class="text-center">Users</h1>

<div class="">
    <table ng-cloak class="table-responsive table-striped table-hover table-bordered">
        <thead>
            <tr>
                <th>Id</th>
                <th>Email</th>
                <th>Email Confirmed</th>
                <th>Access Failed Count</th>
                <th>Lockout Enabled</th>
                <th>Phone Number</th>
                <th>Phone Number Confirmed</th>
                <th>User Name</th>
                @*<th colspan="2"><button type="button" class="btn btn-info" data-toggle="modal" data-target="#newQuestion">New Question</button></th>*@
            </tr>
        </thead>
        <tbody>
            <tr ng-repeat="user in homeCtrl.users">
                <td>{{user.Id}}</td>
                <td>{{user.Email}}</td>
                <td>{{user.EmailConfirmed}}</td>
                <td>{{user.AccessFailedCount}}</td>
                <td>{{user.LockoutEnabled}}</td>
                <td>{{user.PhoneNumber}}</td>
                <td>{{user.PhoneNumberConfirmed}}</td>
                <td>{{user.UserName}}</td>
                @*<td>
                    <button type="button" class="btn btn-warning btn-sm" data-toggle="modal" data-target="#editQuestion" ng-click="quizCtrl.editButton(quiz.Id)">Edit</button>
                    <button class="btn-danger btn-sm" ng-click="quizCtrl.deleteButton(quiz.Id, $index)">Delete</button>
                </td>*@
            </tr>
        </tbody>
    </table>
</div>

